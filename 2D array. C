#include <stdio.h>
int main (){
	int i;
	int j;
	int scores [3] [3]={
		{20,30,50},
		{10,60,35},
		{12,15,56}
	};
	for(i=0; i<3; i++){
		for(j=0; j<3; j++){
			printf("scores [%d] [%d] =%d \n",i ,j,scores[i] [j]);
		}
	}
	
	return 0;
	
}
